<p id="notice"><%= notice %></p>

<h1>Game02s</h1>

<table>
  <thead>
    <tr>
      <th>Rule text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @game02s.each do |game02| %>
      <tr>
        <td><%= game02.rule_text %></td>
        <td><%= link_to 'Show', game02 %></td>
        <td><%= link_to 'Edit', edit_game02_path(game02) %></td>
        <td><%= link_to 'Destroy', game02, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game02', new_game02_path %>
